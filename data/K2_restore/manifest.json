{
  "format": "k2_restore",
  "version": 1,

  "target": {
    "soc": "allwinner-t113",
    "scheme": "swupdate-ab",
    "by_name_base": "/dev/by-name/"
  },

  "profile": "restore_A_from_imgs",

  "stage0_fel": {
    "description": "Boot temporary U-Boot into RAM (no flash writes) so we can safely expose eMMC via UMS or run restore.",
    "filename": "payload/u-boot-sunxi-with-spl.bin",
    "method": "sunxi-fel-spl"
  },

  "images": [
    {
      "name": "bootA",
      "filename": "payload/bootA.img",
      "device": "/dev/by-name/bootA",
      "installed_directly": true,
      "type": "block"
    },
    {
      "name": "rootfsA",
      "filename": "payload/rootfsA.img",
      "device": "/dev/by-name/rootfsA",
      "installed_directly": true,
      "type": "block"
    },

    {
      "name": "env",
      "filename": "payload/env.img",
      "device": "/dev/by-name/env",
      "installed_directly": true,
      "type": "block",
      "optional": true
    },
    {
      "name": "env-redund",
      "filename": "payload/env-redund.img",
      "device": "/dev/by-name/env-redund",
      "installed_directly": true,
      "type": "block",
      "optional": true
    },

    {
      "name": "boot-resource",
      "filename": "payload/boot-resource.img",
      "device": "/dev/by-name/boot-resource",
      "installed_directly": true,
      "type": "block",
      "optional": true
    },

    {
      "name": "uboot",
      "filename": "payload/uboot",
      "type": "awuboot",
      "optional": true
    }
  ],

  "bootenv": [
    { "name": "swu_mode", "value": "" },
    { "name": "boot_partition", "value": "bootA" },
    { "name": "root_partition", "value": "rootfsA" },
    { "name": "swu_next", "value": "reboot" },
    { "name": "version", "value": "1.1.0.94" }
  ],

  "notes": [
    "This manifest is based on the files currently present in payload/ (bootA.img, rootfsA.img, env.img, env-redund.img, boot-resource.img, uboot, u-boot-sunxi-with-spl.bin).",
    "B-slot images are not referenced because they were missing/0-byte in the extraction.",
    "kernel + rootfs (non-.img) are SWUpdate-style payloads and are not used in this direct block restore profile."
  ]
}